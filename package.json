{
    "name": "whichthemeisthat",
    "displayName": "Which Theme Is That?",
    "description": "Shows your current theme and font names on the status bar. Useful when sharing your screen :)",
    "version": "0.0.1",
    "publisher": "sjardim",
    "icon": "images/whichthemeisthat.png",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.whichThemeIsThat"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.whichThemeIsThat",
                "title": "Which Theme is That?"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
